{
  "build": {
    "env": {
      "NPM_TOKEN": "", 
      "TURBO_TOKEN": ""
    }
  },
  
  "projects": [
    {
      "rootDirectory": "apps/dashboard",
      "name": "dashboard"
    },
    {
      "rootDirectory": "apps/network-editor",
      "name": "network-editor"
    }
  ],

  "rewrites": [
    {
      "source": "/network-editor/:path*",
      "destination": "/network-editor/:path*" 
    }
  ]
}